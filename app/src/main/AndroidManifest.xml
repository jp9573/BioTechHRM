<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.jay.setpayscale">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".SetPayScale" />
        <activity android:name=".PostSetting" />
        <activity android:name=".VacancySearch" />
        <activity android:name=".CurrentVacancy" />
        <activity android:name=".CurrentVacancyList" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.category.Default" />
                <action android:name="android.intent.action.MAIN" />
            </intent-filter>
        </activity>
        <activity android:name=".ProfileEdit" />
        <activity android:name=".SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Login" />
        <activity android:name=".SearchEmployee"></activity>
    </application>

</manifest>